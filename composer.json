{
  "name": "baumann-at/php-polygonscan-api",
  "type": "library",
  "description": "PHP client for the Polygonscan API",
  "homepage": "https://github.com/baumann-at/php-polygonscan-api",
  "keywords": ["Polygonscan", "polygon", "api", "Polygonscan.com"],
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Baumann",
      "email": "c.baumann@baumann.at",
      "homepage": "https://github.com/baumann-at"
    }
  ],
  "support": {
    "email": "c.baumann@baumann.at"
  },
  "require": {
    "php": "^5.6 || ^7.0 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5 || ^6.0",
    "mockery/mockery": "~0.8",
    "guzzlehttp/guzzle": "~5.0"
  },
  "autoload": {
    "psr-4": { "Polygonscan\\": "lib/Polygonscan/" }
  },
  "autoload-dev": {
    "psr-4": { "Polygonscan\\Tests\\": "tests/Polygonscan/"}
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
